process {
    executor = 'awsbatch'
    queueSize = 100
    maxForks = 16          
    errorStrategy = 'retry'       
    maxRetries = 20  
    maxErrors = 100
}
env {
    ECS_TASK_METADATA_RPS_LIMIT = "5,10"
}
process.resourceLabels = ['allen-batch-pipeline': 'aind-multiplane-ophys-pipeline-split']